# Generated by go2rpm
%bcond_without check

# https://github.com/hashicorp/raft-boltdb
%global goipath         github.com/hashicorp/raft-boltdb
%global commit          4207f1bf061751378aee6cdfe697965a13ab49d7

%gometa

%global common_description %{expand:
This repository provides the Raftboltdb package. The package exports the
BoltStore which is an implementation of both a LogStore and StableStore.}

%global golicenses      LICENSE
%global godocs          README.md

%global gosupfiles      glide.lock glide.yaml

Name:           %{goname}
Version:        0
Release:        0.20%{?dist}
Summary:        Raft backend implementation using BoltDB

# Upstream license specification: MPL-2.0
License:        MPLv2.0
URL:            %{gourl}
Source0:        %{gosource}
Source1:        glide.yaml
Source2:        glide.lock

BuildRequires:  golang(github.com/boltdb/bolt)
BuildRequires:  golang(github.com/hashicorp/go-msgpack/codec)
BuildRequires:  golang(github.com/hashicorp/raft)

%if %{with check}
# Tests
BuildRequires:  golang(github.com/hashicorp/raft/bench)
%endif

%description
%{common_description}

%gopkg

%prep
%goprep
cp %{S:1} %{S:2} .

%install
%gopkginstall

%if %{with check}
%check
%gocheck
%endif

%gopkgfiles

