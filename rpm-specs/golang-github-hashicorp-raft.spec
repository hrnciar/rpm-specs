# Generated by go2rpm
# fails randomly
%ifarch x86_64
%bcond_without check
%endif
%bcond_with bootstrap

# https://github.com/hashicorp/raft
%global goipath         github.com/hashicorp/raft
Version:                1.1.2

%gometa

%global common_description %{expand:
Golang implementation of the Raft consensus protocol.}

%global golicenses      LICENSE
%global godocs          README.md membership.md CHANGELOG.md

%global gosupfiles      glide.lock glide.yaml

Name:           %{goname}
Release:        2%{?dist}
Summary:        Golang implementation of the Raft consensus protocol

# Upstream license specification: MPL-2.0
License:        MPLv2.0
URL:            %{gourl}
Source0:        %{gosource}
Source1:        glide.yaml
Source2:        glide.lock

BuildRequires:  golang(github.com/armon/go-metrics)
BuildRequires:  golang(github.com/hashicorp/go-hclog)
BuildRequires:  golang(github.com/hashicorp/go-msgpack/codec)

%if %{without bootstrap}
%if %{with check}
# Tests
BuildRequires:  golang(github.com/hashicorp/raft-boltdb)
BuildRequires:  golang(github.com/stretchr/testify/require)
%endif
%endif

%description
%{common_description}

%gopkg

%prep
%goprep
cp %{S:1} %{S:2} .

%install
%gopkginstall

%if %{without bootstrap}
%if %{with check}
%check
# .: needs network
%gocheck -d .
%endif
%endif

%gopkgfiles

