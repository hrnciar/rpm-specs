# Generated by go2rpm
%bcond_without check

# https://github.com/hashicorp/go-msgpack
%global goipath         github.com/hashicorp/go-msgpack
Version:                0.5.5

%gometa

# Remove in F33:
%global godevelheader %{expand:
Obsoletes:      golang-github-hashicorp-go-msgpack-devel < 0-0.16
}

%global common_description %{expand:
Package Codec provides a High Performance, Feature-Rich Idiomatic Go 1.4+
codec/encoding library for binc, msgpack, cbor, json.}

%global golicenses      LICENSE
%global godocs          README.md msgpack.org.md README-codec.md

%global gosupfiles glide.lock glide.yaml

Name:           %{goname}
Release:        4%{?dist}
Summary:        Codec/encoding library for binc, msgpack, cbor, json

# Upstream license specification: BSD-3-Clause
License:        BSD
URL:            %{gourl}
Source0:        %{gosource}
Source1:        glide.yaml
Source2:        glide.lock
Patch0:         0001-Bypass-flag.Parse-in-init.patch

%description
%{common_description}

%gopkg

%prep
%goprep
%patch0 -p1
mv codec/README.md README-codec.md
# Avoid error in %check because of the double commented line
sed -i "s|// //+build ignore|//+build ignore|" codec/ext_dep_test.go
cp %{S:1} %{S:2} .

%install
%gopkginstall

%if %{with check}
%check
%gocheck
%endif

%gopkgfiles

