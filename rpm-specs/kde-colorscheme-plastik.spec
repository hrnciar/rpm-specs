Name:           kde-colorscheme-plastik
Version:        0.1.2
Release:        17%{?dist}
Summary:        Plastik KDE 4 Color Scheme

License:        LGPLv2+
URL:            http://www.kde-look.org/content/show.php/Plastik?content=89804
Source0:        http://kde-look.org/CONTENT/content-files/89804-Plastik.colors
# Description from site
Source1:        README

BuildArch:      noarch

BuildRequires:  kde-filesystem

Requires:       kde-filesystem

%description
This package contains the KDE 4 port of KDE 3 Plastik color scheme.

%prep
%setup -c -T

%build

%install
rm -rf $RPM_BUILD_ROOT
install -p -D -m 644 %{SOURCE0}  $RPM_BUILD_ROOT%{_kde4_datadir}/kde4/apps/color-schemes/Plastik.colors
install -m 644 %{SOURCE1} README

%files
%doc README
%{_kde4_datadir}/kde4/apps/color-schemes/Plastik.colors

