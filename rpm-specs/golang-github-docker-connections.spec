# Generated by go2rpm
%bcond_without check

# https://github.com/docker/go-connections
%global goipath         github.com/docker/go-connections
Version:                0.4.0

%gometa

# Remove in F33:
%global godevelheader %{expand:
Obsoletes:      golang-github-docker-go-connections-devel < 0.4.0-5
}

%global common_description %{expand:
Go-connections provides common package to work with network connections.}

%global golicenses      LICENSE
%global godocs          CONTRIBUTING.md README.md

Name:           %{goname}
Release:        9%{?dist}
Summary:        Utility package to work with network connections

# Upstream license specification: Apache-2.0
License:        ASL 2.0
URL:            %{gourl}
Source0:        %{gosource}

BuildRequires:  golang(github.com/pkg/errors)
BuildRequires:  golang(golang.org/x/net/proxy)

%if %{with check}
# Tests
BuildRequires:  golang(github.com/stretchr/testify/assert)
%endif

%description
%{common_description}

%gopkg

%prep
%goprep

%install
%gopkginstall

%if %{with check}
%check
%gocheck
%endif

%gopkgfiles

