# Generated by go2rpm
%bcond_without check

# https://github.com/inconshreveable/muxado
%global goipath         github.com/inconshreveable/muxado
%global commit          fc182d90f26ea93f36ef05b643c7f4ea604c370b

%gometa

%global common_description %{expand:
Stream multiplexing for Go.}

%global golicenses      LICENSE
%global godocs          README.md TODO

%global gosupfiles      glide.lock glide.yaml

Name:           %{goname}
Version:        0
Release:        0.20%{?dist}
Summary:        Stream multiplexing for Go

# Upstream license specification: Apache-2.0
License:        ASL 2.0
URL:            %{gourl}
Source0:        %{gosource}
Source1:        glide.yaml
Source2:        glide.lock

%if %{with check}
# Tests
BuildRequires:  golang(github.com/hashicorp/yamux)
BuildRequires:  golang(golang.org/x/crypto/ssh)
%endif

%description
%{common_description}

%gopkg

%prep
%goprep
%goprep
cp %{S:1} %{S:2} .

%install
%gopkginstall

# Remove in F33
# Remove erroneous glide.lock folder
%pretrans devel -p <lua>
path = "%{gopath}/src/%{goipath}/glide.lock"
st = posix.stat(path)
if st and st.type == "directory" then
  os.remove(path)
end

%if %{with check}
%check
%gocheck -d .
%endif

%gopkgfiles

