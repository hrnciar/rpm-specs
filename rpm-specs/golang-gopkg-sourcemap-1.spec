# Generated by go2rpm
%bcond_without check

# https://github.com/go-sourcemap/sourcemap
%global goipath         gopkg.in/sourcemap.v1
%global forgeurl        https://github.com/go-sourcemap/sourcemap
Version:                1.0.5

%gometa

# Remove in F33
%global godevelheader %{expand:
Obsoletes:      golang-gopkg-sourcemap-devel < 1.0.5-3
Obsoletes:      golang-gopkg-sourcemap-unit-test-devel < 1.0.5-3
}

%global common_description %{expand:
Source maps consumer for Golang.}

%global golicenses      LICENSE
%global godocs          README.md

Name:           %{goname}
Release:        10%{?dist}
Summary:        Source maps consumer for Golang

# Upstream license specification: BSD-2-Clause
License:        BSD
URL:            %{gourl}
Source0:        %{gosource}

%description
%{common_description}

%gopkg

%prep
%goprep

%install
%gopkginstall

%if %{with check}
%check
%gocheck
%endif

%gopkgfiles

