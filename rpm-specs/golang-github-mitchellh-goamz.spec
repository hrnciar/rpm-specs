%gopkg

%prep
%goprep
%patch0 -p1
find . -name "*.go" -exec sed -i "s|github.com/motain/gocheck|gopkg.in/check.v1|" "{}" +;

%install
%gopkginstall

%if %{with check}
%check
%gocheck -d s3
%endif

%gopkgfiles

