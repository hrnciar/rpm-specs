# Generated by go2rpm
%bcond_without check

# https://github.com/mattn/go-sqlite3
%global goipath         github.com/mattn/go-sqlite3
Version:                1.14.0

%gometa

# Remove in F33
%global godevelheader %{expand:
Obsoletes:      golang-github-mattn-go-sqlite3-devel < 1.10.0-2
}

%global common_description %{expand:
Package Sqlite3 provides interface to SQLite3 databases.}

%global golicenses      LICENSE
%global godocs          _example README.md

%global gosupfiles      glide.lock glide.yaml

Name:           %{goname}
Release:        1%{?dist}
Summary:        Sqlite3 driver for go using database/sql

License:        MIT
URL:            %{gourl}
Source0:        %{gosource}
Source1:        glide.yaml
Source2:        glide.lock

%description
%{common_description}

%gopkg

%prep
%goprep
cp %{S:1} %{S:2} .

%install
%gopkginstall

%if %{with check}
%check
%gocheck
%endif

%gopkgfiles

